<%- model_class = Evaluacion -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:empleado_id) %>:</strong></dt>
  <dd><%= @evaluacion.empleado_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:servicio_id) %>:</strong></dt>
  <dd><%= @evaluacion.servicio_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:observacion) %>:</strong></dt>
  <dd><%= @evaluacion.observacion %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              evaluaciones_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_evaluacion_path(@evaluacion), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              evaluacion_path(@evaluacion),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
